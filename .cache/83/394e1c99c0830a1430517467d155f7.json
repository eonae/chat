fo = (def.info) ? def.info : 'No info for this command';\r\n    \r\n    if (def.aliases) {\r\n      this._aliases = (typeof def.aliases === 'string') ? [def.aliases] : def.aliases;\r\n    } else {\r\n      this._aliases = [];\r\n    }\r\n    \r\n    if (def.flags) {\r\n      def.flags.forEach(f => {\r\n        if (typeof f === 'string') {\r\n          this._flags.push({ name: f, info: 'No info for this flag', aliases: [] })\r\n        } else {\r\n          this._flags.push(f);\r\n        }\r\n      });\r\n    }\r\n  }\r\n\r\n  public bindToTerminal(terminal: ITerminal) {\r\n    if (!this._terminal) this._terminal = terminal;\r\n  }\r\n\r\n  public get flags() : Flag[] {\r\n    return this._flags.slice(0);\r\n  }\r\n  public get aliases() : string[] {\r\n    return this._aliases.slice(0);\r\n  }\r\n\r\n  exec(params: string[], flags: string[]) : Promise<boolean> {\r\n    // Checking flags correctness\r\n    try {\r\n      const resolvedFlags = this._resolveFlags(flags);\r\n    } catch (err) {\r\n      return Promise.reject(err);\r\n    }\r\n    // Validating parameters\r\n    const val = this._validation(params);\r\n    if (!val) return Promise.reject('Invalid parametes'); // Потом надо будет сделать объект ошибки (или сообщение), которое будет вылетать из функции валидации.\r\n\r\n    return this._action(params, flags, this._terminal);\r\n  }\r\n\r\n  private _resolveFlags(flags: string[]) : Flag[] {\r\n\r\n      return flags.map(flag => {\r\n        const findByName = this._flags.find(f => f.name === flag);\r\n        if (findByName) return findByName;\r\n        else {\r\n          const findByAlias = this._flags.find(f => f.aliases.indexOf(flag) !== -1);\r\n          if (findByAlias) return findByAlias;\r\n          else throw new Error('Invalid flag!');\r\n        }\r\n      });\r\n  }\r\n}\r\n\r\nexport { CommandConfiguration, Flag, Command };"},"lineCount":null}},"error":null,"hash":"6986348e0f6ece0d81f6f50c2a6f9a93","cacheData":{"env":{}}}