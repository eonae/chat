pps/shell/CommandTrigger.ts","name":null,"original":{"line":1,"column":0},"generated":{"line":60,"column":18}},{"source":"ts/apps/shell/CommandTrigger.ts","name":null,"original":{"line":1,"column":0},"generated":{"line":60,"column":32}}],"sources":{"ts/apps/shell/CommandTrigger.ts":"export default class CommandTrigger {\r\n\r\n  public command: string = '';\r\n  public arguments: string[] = [];\r\n  public flags: string[] = [];\r\n\r\n  public static getFrom(source: string) : CommandTrigger {\r\n    const result = new CommandTrigger();\r\n    const arr = source.trim().split(' ');\r\n    result.command = arr[0];\r\n    arr.splice(0, 1);\r\n    result.flags = CommandTrigger.refineFlags(arr.filter(item => CommandTrigger.isFlag(item)));\r\n    result.arguments = arr.filter(item => !CommandTrigger.isFlag(item));\r\n\r\n    return result;\r\n  }\r\n\r\n  private static isFlag(value: string) : boolean {\r\n    return /^\\-{1,2}/.test(value);\r\n  }\r\n  \r\n  private static refineFlags(flags: string[]) : string[] {\r\n    let result: string[] = [];\r\n    flags.forEach(f => {\r\n      const temp = f.substr(1); // Удаляем первый слэш\r\n      if (temp.substr(0, 1) === '-') result.push(temp.substr(1));\r\n      else result = result.concat(temp.split(''));\r\n    })\r\n    return result;\r\n  }\r\n\r\n}"},"lineCount":null}},"error":null,"hash":"de01f475c3dd5dfd20eae08afcd421b1","cacheData":{"env":{}}}